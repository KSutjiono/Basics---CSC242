//****ANSWER BELOW
#include <iostream>
#include <cstdlib> //FOR RAND AND SRAND FUNCTIONS
#include <ctime> //FOR TIME FUNCTION
using namespace std;
int main(){
//Initial input of 2 chosen letter characters
  cout << "Enter a starting character: ";
  char startChar;
  cin >> startChar;
  cout << "Enter an ending character: ";
  char endChar;
  cin >> endChar;
  
//Get a random character
  srand(time(0));
  char randomChar = static_cast<char>(startChar + rand() % 
    (endChar - startChar + 1));
    
//Output
  cout << "The random character between " << startChar << " and " 
       << endChar << " is " << randomChar << endl;

  return 0;}
